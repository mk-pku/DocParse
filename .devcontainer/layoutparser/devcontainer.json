{
  "name": "DocParse - LayoutParser",
  "dockerComposeFile": [
    "../../docker-compose.lp.yml"
  ],
  "service": "layoutparser",
  "workspaceFolder": "/app",
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python",
        "charliermarsh.ruff"
      ],
      "settings": {
        "editor.formatOnSave": true,
        "[python]": {
          "editor.defaultFormatter": "charliermarsh.ruff",
        },
      }
    }
  }
}
